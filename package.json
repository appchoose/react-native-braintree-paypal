{
  "name": "@appchoose/react-native-braintree-paypal",
  "version": "0.3.6",
  "description": "RN Braintree for PayPal payment",
  "main": "./lib/module/index.js",
  "types": "./lib/typescript/src/index.d.ts",
  "exports": {
    ".": {
      "source": "./src/index.tsx",
      "types": "./lib/typescript/src/index.d.ts",
      "default": "./lib/module/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "src",
    "lib",
    "android",
    "ios",
    "cpp",
    "*.podspec",
    "react-native.config.js",
    "!ios/build",
    "!android/build",
    "!android/gradle",
    "!android/gradlew",
    "!android/gradlew.bat",
    "!android/local.properties",
    "!**/.*"
  ],
  "scripts": {
    "lint": "npx biome check .",
    "lint:fix": "npx biome check --write .",
    "clean": "del-cli android/build lib",
    "prepare": "bob build",
    "release": "semantic-release"
  },
  "keywords": ["react-native", "ios", "android", "braintree", "paypal"],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/appchoose/react-native-braintree-paypal.git"
  },
  "author": "Hugo EXTRAT <extrat.h@gmail.com> (https://github.com/huextrat)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/appchoose/react-native-braintree-paypal/issues"
  },
  "homepage": "https://github.com/appchoose/react-native-braintree-paypal#readme",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@commitlint/cli": "19.8.1",
    "@commitlint/config-conventional": "19.8.1",
    "@react-native-community/cli": "15.1.3",
    "@react-native/babel-preset": "0.79.2",
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/git": "10.0.1",
    "@types/react": "19.0.0",
    "del-cli": "6.0.0",
    "react": "19.0.0",
    "react-native": "0.79.2",
    "react-native-builder-bob": "0.40.11",
    "semantic-release": "24.2.5",
    "turbo": "2.5.3",
    "typescript": "5.8.3"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*"
  },
  "packageManager": "yarn@4.9.1",
  "commitlint": {
    "extends": ["@commitlint/config-conventional"]
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      [
        "module",
        {
          "esm": true
        }
      ],
      [
        "typescript",
        {
          "project": "tsconfig.build.json"
        }
      ]
    ]
  },
  "codegenConfig": {
    "name": "BraintreePaypalSpec",
    "type": "modules",
    "jsSrcsDir": "src",
    "android": {
      "javaPackageName": "com.braintreepaypal"
    }
  },
  "create-react-native-library": {
    "languages": "kotlin-objc",
    "type": "turbo-module",
    "version": "0.50.3"
  }
}
